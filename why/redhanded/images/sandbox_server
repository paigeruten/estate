            if (ss.addRule)\n                    {\n                        ss.addRule(sel, prop);\n                    }\n                    else if (ss.insertRule)\n                    {\n                        ss.insertRule(sel+\" { \" +prop+\" }\", 0);\n                    }\n                }\n                injectCleanStylesFor(\"balloonEmbedd1\", \"balloonEmbedd2\");\n            </script><div id=\"balloonEmbedd1\"><div id=\"balloonEmbedd2\"><h1><a href=\"http://balloon.hobix.com/sandbox_server\">sandbox_server</a>, a balloon:</h1><p>Serve Ruby sessions over a socket.</p><div><input name=\"balloon_sandbox_server\" value=\"ruby -ropen-uri -e 'eval(open(\"http://balloon.hobix.com/sandbox_server\").read)'\" readonly=\"true\" type=\"text\"/></div></div></div></div>")